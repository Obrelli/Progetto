<table class="table table-striped table-dark">
    <thead class="thead-dark">
        <tr>
            <th>Tipo servizio</th>
            <th>Prezzo servizio</th>
          </tr>
    </thead>
    <tbody>
        <% servizi.forEach(servizio=> { %>
            <tr onclick="idservizio('<%= servizio.id_servizio %>')">
            <!-- <tr action="/modificaservizio?id='<%= servizio.id_servizio %>'" value="<%= servizio.id_servizio %>"> -->
                <td>
                    <%= servizio.tipo_servizio %>
                </td>
                <td>
                    <%= servizio.prezzo_servizio %>
                </td>
            </tr>
            <% }) %>
    </tbody>
  </table>