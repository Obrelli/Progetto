<table class="table table-striped table-dark">
    <thead class="thead-dark">
        <tr>
            <th scope="col">ID</th>
            <th scope="col">Name</th>
        </tr>
    </thead>
    <tbody>
        <% departments.forEach(department=> { %>
            <tr>
                <td>
                    <%= department._id %>
                </td>
                <td>
                    <%= department.name %>
                </td>
            </tr>
            <% }) %>
    </tbody>
</table>