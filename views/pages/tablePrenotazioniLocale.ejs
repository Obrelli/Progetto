

<table class="table table-striped table-dark">
    <thead class="thead-dark">
        <tr>
            <th>Nome Tipologia</th>
            <th>Cognome cliente</th>
            <th>Data prenotazione</th>
          </tr>
    </thead>
    <tbody>
        
        <% prenotazioni.forEach(prenotazioni=> { %>
            <tr>
                <td>
                    <%= prenotazioni.nome_tipologia %>
                </td>
                <td>
                    <%= prenotazioni.cognome %>
                </td>
                <td>
                    <%= prenotazioni.data_prenotazione %>
                </td>
                <td>
                    <button type="submit"   method="POST" onclick="location.href='/prenotazioni/prenotazione_spec/<%= prenotazioni.id_prenotazione %>'">Visualizza</button>
                </td>
            </tr>
            <% }) %>
    </tbody>
</table>